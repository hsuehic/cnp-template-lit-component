{
  "name": "wc-progress-bar",
  "version": "1.1.0",
  "description": "Progress bar web component",
  "main": "./dist/progress-bar.js",
  "module": "./dist/progress-bar.js",
  "type": "module",
  "files": [
    "dist",
    "assets"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf progress-bar.{d.ts,d.ts.map,js,js.map} test/progress-bar.{d.ts,d.ts.map,js,js.map} test/progress-bar_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/progress-bar.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds  --node-resolve --watch --hmr",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat progress-bar.bundled.js | gzip -9 | wc -c ; rm progress-bar.bundled.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "MODE=production build-storybook -o ./static",
    "build-storybook:gitlab": "MODE=gitlab build-storybook -o ./public",
    "deploy-storybook": "MODE=production storybook-to-ghpages"
  },
  "keywords": [
    "create npm packages",
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "dependencies": {
    "lit": "^2.2.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@babel/core": "^7.21.0",
    "@changesets/cli": "^2.26.2",
    "@custom-elements-manifest/analyzer": "^0.6.3",
    "@open-wc/testing": "^3.1.5",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^5.0.2",
    "@storybook/addon-actions": "^6.5.16",
    "@storybook/addon-essentials": "^7.1.1",
    "@storybook/addon-links": "^6.5.16",
    "@storybook/addon-storysource": "^6.5.16",
    "@storybook/builder-vite": "^7.1.1",
    "@storybook/builder-webpack4": "^6.5.16",
    "@storybook/manager-webpack4": "^6.5.16",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/web-components": "^7.1.1",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@web/dev-server": "^0.1.31",
    "@web/dev-server-esbuild": "^0.3.3",
    "@web/dev-server-legacy": "^1.0.0",
    "@web/test-runner": "^0.15.0",
    "@web/test-runner-playwright": "^0.9.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "@whitespace/storybook-addon-html": "^5.1.4",
    "babel-loader": "^8.3.0",
    "eslint": "^8.46.0",
    "eslint-config-cnp": "^1.0.0",
    "lit-analyzer": "^0.0.6",
    "lit-html": "^2.6.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.5",
    "react-syntax-highlighter": "^15.5.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.73.0",
    "rollup-plugin-summary": "^1.4.3",
    "rollup-plugin-terser": "^7.0.2",
    "storybook-design-token": "^2.9.0",
    "typescript": "~4.7.4",
    "vite": "^4.1.4"
  },
  "customElements": "custom-elements.json",
  "engines": {
    "node": "16.18.0"
  }
}
